AAA,CAAA,Q,GAEd,EADF,YAAA,CAAoD,KAApD,EAAoD;AAA/C,IAAA,KAAK,EAAC,cAAyC;AAAzB,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,aAAE,QAAA,CAAA,OAAA,EAAF;AAAA,KAAA;AAAoB,GAApD,EAA6C,GAA7C,CACE,CAHN,CAgBE,EAZF,YAAA,CAWM,KAXN,EAAA,UAAA,EAWM,CAVF,YAAA,CAGM,KAHN,EAAA,UAAA,EAGM,CAFF,mBAAA,CAAA,6CAAA,CAEE,EADF,YAAA,CAAyD,sBAAzD,EAAyD;AAApC,IAAA,UAAU,EAAE,KAAA,CAAA,QAAwB;;aAAxB,KAAA,CAAA,QAAA,GAAQ,M;;AAAgB,GAAzD,E,IAAA,E;;AAAA,I,cAAA,CACE,CAHN,CAUE,EAJF,YAAA,CAGM,KAHN,EAAA,UAAA,EAGM,CAFF,mBAAA,CAAA,6CAAA,CAEE,EADF,YAAA,CAAuD,oBAAvD,EAAuD;AAAnC,IAAA,UAAU,EAAE,KAAA,CAAA,SAAuB;;aAAvB,KAAA,CAAA,SAAA,GAAS,M;;AAAc,GAAvD,E,IAAA,E;;AAAA,I,cAAA,CACE,CAHN,CAIE,CAXN,CAYE,CAjBN,C","sourcesContent":["<template>\n    <div class=\"task-header\">\n        <div class=\"task-input\">\n            <input v-model=\"taskName\" placeholder=\"Write Something\" @keypress.enter=\"addTask()\">\n            <div class=\"task-add-btn\" @click=\"addTask()\">+</div>\n        </div>\n        <div class=\"task-options\">\n            <div class=\"task-catagory\">\n                <!--<p class=\"taskOption-label\">Category:</p>-->\n                <ColorPicker v-model:radioValue=\"selected\"></ColorPicker>\n            </div>\n\n            \n            <div class=\"task-duedate\">\n                <!--<p class=\"taskOption-label\">Due Date:</p>-->\n                <DateInput  v-model:modelValue=\"dateValue\"></DateInput>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\nimport DateInput from '@/components/DatePicker'\nimport ColorPicker from '@/components/ColorPicker'\nimport axios from 'axios'\nexport default {\n    components:{\n        DateInput,\n        ColorPicker\n    },\n    data(){\n        return{\n            taskName:'',\n            taskList:null,\n            taskIndex:0,\n            dateValue:'',\n            selected:'',\n            types:\n                [{'typeName':'Business','color':'#6FCF97'},\n                {'typeName':'Personal','color':'#524EEE'},\n                {'typeName':'Important','color':'#D06969'}]\n        }\n    },\n    methods:{\n        addTask(){\n            if(this.taskName.trim().length===0){\n                return\n            }\n\n            let timeStamp = Math.floor(Date.now()) \n\n            \n            let item = {'id':timeStamp ,'name':this.taskName,'category':this.selected,'duedate':this.dateValue,'isDone':false}\n            \n            axios.post('http://localhost:3000/taskList' ,item)\n            .then((response)=>{this.taskList.push(response.data)})\n            .catch((error)=>{console.log(error)})\n            \n            this.taskName=''\n\n        },\n    }\n}\n</script>\n\n<style scoped>\n\n</style>"],"sourceRoot":""}]}