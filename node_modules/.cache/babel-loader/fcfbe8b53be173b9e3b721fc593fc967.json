MAAA,IAAI,EAAC,UAAuE;;eAAnD,IAAI,CAAC,MAAL,GAAW,M;OAAwC;AAArC,MAAA,EAAE,EAAE,IAAI,CAAC,EAA4B;AAAvB,MAAA,OAAK,EAAA,iBAAA,MAAA;AAAA,eAAE,QAAA,CAAA,QAAA,CAAS,IAAT,CAAF;AAAA;AAAkB,KAAnF,E,IAAA,E;;AAAA,M,wCAAA,C,qBAAgC,IAAI,CAAC,M,GAEnC,EADF,YAAA,CAAkM,OAAlM,EAAkM;AAA1L,MAAA,GAAG,EAAE,IAAI,CAAC,EAAgL;AAA5K,MAAA,KAAK,EAAA,CAAC,0BAAD,EAA2B;AAAA,QAAA,MAAA,EAAiB,IAAI,CAAC;AAAtB,OAA3B;AAAuK,KAAlM,E,CAAqF,YAAA,CAAwF,MAAxF,EAAwF;AAAjF,MAAA,GAAG,EAAE,IAAI,CAAC,QAAuE;AAA5D,MAAA,KAAK,EAAA;AAAA,QAAA,WAAA,YAAkB,QAAA,CAAA,UAAA,CAAW,IAAI,CAAC,QAAhB,CAAlB;AAAA;AAAuD,KAAxF,E,IAAA,E;;AAAA,M,OAAA,C,oCAA0F,IAAI,CAAC,I,GAAI;AAAA;MAAxL,E;;AAAA,M,OAAA,CACE,CAHN,C,EAIA,YAAA,CAGM,KAHN,EAAA,UAAA,EAGM,CAFF,mBAAA,CAAA,+BAAA,CAEE,EADF,YAAA,CAAgI,KAAhI,EAAgI;AAA3H,MAAA,KAAK,EAAA,CAAC,QAAD,EAAS;AAAA,QAAA,UAAA,EAAqB,IAAI,CAAC;AAA1B,OAAT,CAAsH;AAAzE,MAAA,OAAK,EAAA,iBAAA,MAAA;AAAA,eAAE,QAAA,CAAA,UAAA,CAAW,IAAI,CAAC,EAAhB,CAAF;AAAA;AAAoE,KAAhI,E,CAAmF,U,CAAnF,E;;AAAA,M,WAAA,CACE,CAHN,C,CALJ,C;GAUM,CAVN,E;;AAAA,GAWE,EAZN,C","sourcesContent":["<template>\n    <div class=\"task-list\">\n        <div class=\"task-item\" v-for=\"task in filterList\" :key=\"task.id\">\n            <div class=\"task-item-left\">\n                <input type=\"checkbox\" v-model=\"task.isDone\" :id=\"task.id\" @click=\"markDone(task)\">\n                <label :for=\"task.id\" class=\"task-item-name text-wrap\" :class=\"{isDone:task.isDone}\"><span :for=\"task.category\" :style=\"{borderColor:`${indexColor(task.category)}`}\"></span>{{task.name}}</label>\n            </div>\n            <div class=\"task-item-right\">\n                <!-- <div>{{task.duedate}}</div> -->\n                <div class=\"remove\" :class=\"{showRemove:task.isDone}\" @click=\"removeTask(task.id)\"><img src=\"../assets/images/remove.svg\"></div>\n            </div>\n           \n        </div>\n    </div>        \n</template>\n\n<script>\nimport axios from 'axios'\nexport default {\n    props:{filterValue:String},\n    data(){\n        return{\n            taskList: [],\n            filterList:[],\n            // filterValue:'',\n            types:\n                [{'typeName':'Business','color':'#6FCF97'},\n                {'typeName':'Personal','color':'#524EEE'},\n                {'typeName':'Important','color':'#D06969'}],\n            isFiltered: false\n        }\n    },\n    created(){\n        this.getTask()\n    },\n    watch:{\n        filterValue:{\n            deep:true,\n            handler(newVal){\n                this.filterCat(newVal)\n            }\n        }\n    },\n    methods:{\n        getTask(){\n            axios.get('http://localhost:3000/taskList/')\n            .then(response=>{\n\n                this.taskList = response.data;\n                this.filterList = this.taskList;\n            })\n            .catch(()=>{\n                alert('This App requires JSON Server Running')\n            })\n        },\n\n        removeTask(index){\n            axios.delete('http://localhost:3000/taskList/' + index)\n            .then(() => {this.taskList.splice(index,1)})\n            .catch((error)=>{console.log(error)});\n            \n            this.getTask()\n        },\n\n        markDone(item){\n            if(item.isDone === false){\n                axios.patch('http://localhost:3000/taskList/' + item.id, {isDone: true})\n                .then(()=> {})\n                .catch((error)=>{console.log(error)});\n            }else{\n            axios.patch('http://localhost:3000/taskList/' + item.id, {isDone: false})\n            .then(()=> {})\n            .catch((error)=>{console.log(error)});}\n        },\n\n        indexColor(index){\n            if(!index){return '#4E2ECF'}\n            let color = this.types.find( element=>element.typeName === index).color\n            return color\n        },\n        \n        filterCat(value){\n            this.filterList = this.taskList;\n            let filterArray=[];\n            if(!value){filterArray = this.filterList}\n            else{\n            filterArray = this.filterList.filter(item=>{return item.category.includes(value)})}\n            this.filterList = filterArray;\n        }\n        \n    },\n}\n</script>\n\n<style scoped>\n\n    .task-list{\n        width:100%;\n    }\n\n    .task-item{\n        display: flex;\n        box-sizing: border-box;\n        background-color: var(--highlight);\n        height:75px;\n        border-radius: 1em;\n        padding:1.5em;\n        justify-content: space-between;\n        align-items: center;\n        margin-bottom: 1em;\n    }\n\n    .task-item-name{\n        font-weight:300;\n        font-size:1.2em;\n        color:var(--white);\n        margin:auto 0;\n    }\n\n    .task-item-right{\n        display:inline-flex;\n        align-self: center;\n    }\n\n\n    input[type=\"checkbox\"]{\n        display:none;\n    }\n\n    input[type=\"checkbox\"] + label span {\n        display: inline-flex;\n        box-sizing: border-box;\n        width:21px;\n        height:21px;\n        border:3px solid;\n        border-color:var(--contrast);\n        border-radius:6px;\n        cursor:pointer;\n        margin-right:0.5em;\n    }\n\n    input[type=\"checkbox\"]:checked + label span{\n        background:url(\"../assets/images/check.svg\") 1px 2px no-repeat, #42416C;\n    }\n\n    .remove{\n        display: none;\n        margin-left:1em;\n    }\n\n    .showRemove{\n        display: block;\n    }\n\n    .isDone{\n        text-decoration: line-through;\n        color:var(--disabled)\n    }\n\n    @media screen and (max-width:768px){\n        .task-item{\n            padding:1em;\n        }\n\n        .task-item-name{\n            font-size:1em;\n            word-wrap:normal\n        }\n    }\n</style>"],"sourceRoot":""}]}