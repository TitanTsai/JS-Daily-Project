,CAAC,UAAL,GAAkB,KAAI,CAAC,QAAvB;AACH,OALD,EAMC,KAND,CAMO,YAAI;AACP,QAAA,KAAK,CAAC,uCAAD,CAAL;AACH,OARD;AASH,KAXG;AAaJ,IAAA,UAbI,sBAaO,KAbP,EAaa;AAAA;;AACb,MAAA,KAAK,CAAC,MAAN,CAAa,oCAAoC,KAAjD,EACC,IADD,CACM,YAAM;AAAC,QAAA,MAAI,CAAC,QAAL,CAAc,MAAd,CAAqB,KAArB,EAA2B,CAA3B;AAA8B,OAD3C,EAEC,KAFD,CAEO,UAAC,KAAD,EAAS;AAAC,QAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AAAmB,OAFpC;AAIA,WAAK,OAAL;AACH,KAnBG;AAqBJ,IAAA,QArBI,oBAqBK,IArBL,EAqBU;AACV,UAAG,IAAI,CAAC,MAAL,KAAgB,KAAnB,EAAyB;AACrB,QAAA,KAAK,CAAC,KAAN,CAAY,oCAAoC,IAAI,CAAC,EAArD,EAAyD;AAAC,UAAA,MAAM,EAAE;AAAT,SAAzD,EACC,IADD,CACM,YAAK,CAAE,CADb,EAEC,KAFD,CAEO,UAAC,KAAD,EAAS;AAAC,UAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AAAmB,SAFpC;AAGH,OAJD,MAIK;AACL,QAAA,KAAK,CAAC,KAAN,CAAY,oCAAoC,IAAI,CAAC,EAArD,EAAyD;AAAC,UAAA,MAAM,EAAE;AAAT,SAAzD,EACC,IADD,CACM,YAAK,CAAE,CADb,EAEC,KAFD,CAEO,UAAC,KAAD,EAAS;AAAC,UAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AAAmB,SAFpC;AAEsC;AACzC,KA9BG;AAgCJ,IAAA,UAhCI,sBAgCO,KAhCP,EAgCa;AACb,UAAG,CAAC,KAAJ,EAAU;AAAC,eAAO,SAAP;AAAgB;;AAC3B,UAAI,KAAI,GAAI,KAAK,KAAL,CAAW,IAAX,CAAiB,UAAA,OAAO;AAAA,eAAE,OAAO,CAAC,QAAR,KAAqB,KAAvB;AAAA,OAAxB,EAAsD,KAAlE;AACA,aAAO,KAAP;AACH,KApCG;AAsCJ,IAAA,SAtCI,qBAsCM,KAtCN,EAsCY;AACZ,WAAK,UAAL,GAAkB,KAAK,QAAvB;AACA,UAAI,WAAW,GAAC,EAAhB;;AACA,UAAG,CAAC,KAAJ,EAAU;AAAC,QAAA,WAAU,GAAI,KAAK,UAAnB;AAA6B,OAAxC,MACI;AACJ,QAAA,WAAU,GAAI,KAAK,UAAL,CAAgB,MAAhB,CAAuB,UAAA,IAAI,EAAE;AAAC,iBAAO,IAAI,CAAC,QAAL,CAAc,QAAd,CAAuB,KAAvB,CAAP;AAAqC,SAAnE,CAAd;AAAkF;;AAClF,WAAK,UAAL,GAAkB,WAAlB;AACJ;AA7CI;AAfG,CAAf","sourcesContent":["<template>\n    <div class=\"task-list\">\n        <div class=\"task-item\" v-for=\"task in filterList\" :key=\"task.id\">\n            <div class=\"task-item-left\">\n                <input type=\"checkbox\" v-model=\"task.isDone\" :id=\"task.id\" @click=\"markDone(task)\">\n                <label :for=\"task.id\" class=\"task-item-name\" :class=\"{isDone:task.isDone}\"><span :for=\"task.category\" :style=\"{borderColor:`${indexColor(task.category)}`}\"></span>{{task.name}}</label>\n            </div>\n            <div class=\"task-item-right\">\n                <div>{{task.duedate}}</div>\n                <div class=\"remove\" :class=\"{showRemove:task.isDone}\" @click=\"removeTask(task.id)\"><img src=\"../assets/images/remove.svg\"></div>\n            </div>\n            \n        </div>\n    </div>        \n</template>\n\n<script>\nimport axios from 'axios'\nexport default {\n    data(){\n        return{\n            taskList: [],\n            filterList:[],\n            types:\n                [{'typeName':'Business','color':'#6FCF97'},\n                {'typeName':'Personal','color':'#524EEE'},\n                {'typeName':'Important','color':'#D06969'}],\n            isFiltered: false\n        }\n    },\n    created(){\n        this.getTask()\n    },\n    methods:{\n        getTask(){\n            axios.get('http://localhost:3000/taskList/')\n            .then(response=>{\n\n                this.taskList = response.data;\n                this.filterList = this.taskList;\n            })\n            .catch(()=>{\n                alert('This App requires JSON Server Running')\n            })\n        },\n\n        removeTask(index){\n            axios.delete('http://localhost:3000/taskList/' + index)\n            .then(() => {this.taskList.splice(index,1)})\n            .catch((error)=>{console.log(error)});\n            \n            this.getTask()\n        },\n\n        markDone(item){\n            if(item.isDone === false){\n                axios.patch('http://localhost:3000/taskList/' + item.id, {isDone: true})\n                .then(()=> {})\n                .catch((error)=>{console.log(error)});\n            }else{\n            axios.patch('http://localhost:3000/taskList/' + item.id, {isDone: false})\n            .then(()=> {})\n            .catch((error)=>{console.log(error)});}\n        },\n\n        indexColor(index){\n            if(!index){return '#4E2ECF'}\n            let color = this.types.find( element=>element.typeName === index).color\n            return color\n        },\n        \n        filterCat(value){\n            this.filterList = this.taskList;\n            let filterArray=[];\n            if(!value){filterArray = this.filterList}\n            else{\n            filterArray = this.filterList.filter(item=>{return item.category.includes(value)})}\n            this.filterList = filterArray;\n        }\n        \n    },\n}\n</script>\n\n<style scoped>\n\n    .task-list{\n        width:480px\n    }\n\n    .task-item{\n        display: flex;\n        box-sizing: border-box;\n        background-color: var(--highlight);\n        width:480px;\n        height:75px;\n        border-radius: 1em;\n        padding:1.5em;\n        justify-content: space-between;\n        align-items: center;\n        margin-bottom: 1em;\n    }\n\n    .task-item-name{\n        font-weight:300;\n        font-size:1.2em;\n        color:var(--white);\n        margin:auto 0;\n    }\n\n    .task-item-right{\n        display:inline-flex;\n        align-self: center;\n    }\n\n\n    input[type=\"checkbox\"]{\n        display:none;\n    }\n\n    input[type=\"checkbox\"] + label span {\n        display: inline-flex;\n        box-sizing: border-box;\n        width:21px;\n        height:21px;\n        border:3px solid;\n        border-color:var(--contrast);\n        border-radius:6px;\n        cursor:pointer;\n        margin-right:0.5em;\n    }\n\n    input[type=\"checkbox\"]:checked + label span{\n        background:url(\"../assets/images/check.svg\") 1px 2px no-repeat, #42416C;\n    }\n\n    .remove{\n        display: none;\n        margin-left:1em;\n    }\n\n    .showRemove{\n        display: block;\n    }\n\n    .isDone{\n        text-decoration: line-through;\n        color:var(--disabled)\n    }\n\n</style>"],"sourceRoot":""}]}